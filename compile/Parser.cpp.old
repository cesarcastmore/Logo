namespace System {


void Parser::SynErr(int n) {
	if (errDist >= minErrDist) errors->SynErr(la->line, la->col, n);
	errDist = 0;
}

void Parser::SemErr(const wchar_t* msg) {
	if (errDist >= minErrDist) errors->Error(t->line, t->col, msg);
	errDist = 0;
}

void Parser::Get() {
	for (;;) {
		t = la;
		la = scanner->Scan();
		if (la->kind <= maxT) { ++errDist; break; }

		if (dummyToken != t) {
			dummyToken->kind = t->kind;
			dummyToken->pos = t->pos;
			dummyToken->col = t->col;
			dummyToken->line = t->line;
			dummyToken->next = NULL;
			coco_string_delete(dummyToken->val);
			dummyToken->val = coco_string_create(t->val);
			t = dummyToken;
		}
		la = t;
	}
}

void Parser::Expect(int n) {
	if (la->kind==n) Get(); else { SynErr(n); }
}

void Parser::ExpectWeak(int n, int follow) {
	if (la->kind == n) Get();
	else {
		SynErr(n);
		while (!StartOf(follow)) Get();
	}
}

bool Parser::WeakSeparator(int n, int syFol, int repFol) {
	if (la->kind == n) {Get(); return true;}
	else if (StartOf(repFol)) {return false;}
	else {
		SynErr(n);
		while (!(StartOf(syFol) || StartOf(repFol) || StartOf(0))) {
			Get();
		}
		return StartOf(syFol);
	}
}

void Parser::PROGRAMA() {
		Expect(5 /* "program" */);
		Expect(6 /* "{" */);
		InitDeclarations(); 
		action->beginProgram(); 
		while (la->kind == 8 /* "global" */) {
			GLOBAL();
		}
		BLOQUE();
		Expect(7 /* "}" */);
		action->endProgram();
		action->tab->removeAllLocals();
		action->showCuadruplo();
		/*action->tab->displayGlobals();*/
		
		action->createObject();
}

void Parser::GLOBAL() {
		wchar_t* name; int type; 
		Expect(8 /* "global" */);
		TIPO(type);
		IDENTI(name);
		action->addGlobal(name, type, var);  
		if (la->kind == 11 /* "[" */) {
			ARREGLO_INIT();
			action->addDimensionGlobal(name); 
		}
		while (la->kind == 9 /* "," */) {
			Get();
			IDENTI(name);
			action->addGlobal(name, type, var); 
			if (la->kind == 11 /* "[" */) {
				ARREGLO_INIT();
				action->addDimensionGlobal(name); 
			}
		}
		Expect(10 /* ";" */);
}

void Parser::BLOQUE() {
		while (la->kind == 19 /* "module" */) {
			MODULE();
		}
		MAIN();
}

void Parser::TIPO(int  &type) {
		if (la->kind == 14 /* "int" */) {
			Get();
			type =entero; 
		} else if (la->kind == 15 /* "float" */) {
			Get();
			type=flotante; 
		} else SynErr(65);
}

void Parser::IDENTI(wchar_t* &name) {
		Expect(_id);
		name =  coco_string_create(t->val); 
}

void Parser::ARREGLO_INIT() {
		double inferior, superior; 
		Expect(11 /* "[" */);
		action->isDimension(); action->getDimension(); 
		Expect(_integer);
		inferior=wcstof((coco_string_create(t->val)), NULL); action->saveInferior((int)(inferior));  
		Expect(12 /* ".." */);
		Expect(_integer);
		superior=wcstof((coco_string_create(t->val)), NULL); action->saveSuperior((int)(superior));  
		while (la->kind == 9 /* "," */) {
			Get();
			action->getNewDimension(); 
			Expect(_integer);
			inferior=wcstof((coco_string_create(t->val)), NULL); action->saveInferior((int)(inferior));  
			Expect(12 /* ".." */);
			Expect(_integer);
			superior=wcstof((coco_string_create(t->val)), NULL); action->saveSuperior((int)(superior));  
		}
		action->getNewDimension();  action->getKCons(); 
		Expect(13 /* "]" */);
}

void Parser::MODULE() {
		wchar_t* name; int typemod; int no; int can; 
		Expect(19 /* "module" */);
		TIPO_MOD(typemod);
		IDENTI(name);
		action->addGlobal(name, undefined, function); action->addNameFunction(name);
		action->addTypeFunction(typemod); 
		Expect(17 /* "(" */);
		if (la->kind == 14 /* "int" */ || la->kind == 15 /* "float" */) {
			PARAMETROS();
		}
		action->addNoParameters(); 
		Expect(18 /* ")" */);
		Expect(6 /* "{" */);
		while (la->kind == 14 /* "int" */ || la->kind == 15 /* "float" */) {
			LOCAL();
		}
		action->addNoLocals(); 
		action->addContCuadruplo(); 
		while (StartOf(1)) {
			ESTATUTO();
		}
		if (la->kind == 21 /* "return" */) {
			RETORNO();
		}
		action->generateRetorno(); 
		Expect(7 /* "}" */);
		action->removeLocals(); 
}

void Parser::MAIN() {
		Expect(16 /* "main" */);
		Expect(17 /* "(" */);
		Expect(18 /* ")" */);
		int can;   
		Expect(6 /* "{" */);
		action->beginMain(); 
		while (la->kind == 14 /* "int" */ || la->kind == 15 /* "float" */) {
			LOCAL();
		}
		while (StartOf(1)) {
			ESTATUTO();
		}
		Expect(7 /* "}" */);
		action->removeLocals(); 
}

void Parser::LOCAL() {
		wchar_t* name; int type; 
		TIPO(type);
		IDENTI(name);
		action->addLocal(name, type, var);  
		if (la->kind == 11 /* "[" */) {
			ARREGLO_INIT();
			action->addDimensionLocal(name); 
		}
		while (la->kind == 9 /* "," */) {
			Get();
			IDENTI(name);
			action->addLocal(name, type, var); 
			if (la->kind == 11 /* "[" */) {
				ARREGLO_INIT();
				action->addDimensionLocal(name); 
			}
		}
		Expect(10 /* ";" */);
}

void Parser::ESTATUTO() {
		switch (la->kind) {
		case 29 /* "read" */: {
			LECTURA();
			break;
		}
		case 32 /* "draw" */: {
			GRAFICO();
			break;
		}
		case _id: {
			ASIGMODULO();
			break;
		}
		case 24 /* "if" */: {
			CONDICION();
			break;
		}
		case 30 /* "print" */: {
			ESCRITURA();
			break;
		}
		case 27 /* "while" */: {
			CICLO();
			break;
		}
		case 28 /* "do" */: {
			CICLO_DO();
			break;
		}
		default: SynErr(66); break;
		}
}

void Parser::TIPO_MOD(int &typemod) {
		if (la->kind == 14 /* "int" */ || la->kind == 15 /* "float" */) {
			int type;  
			TIPO(type);
			typemod = type; 
		} else if (la->kind == 20 /* "void" */) {
			Get();
			typemod = undefined; 
		} else SynErr(67);
}

void Parser::PARAMETROS() {
		PARAM_COMA();
		while (la->kind == 9 /* "," */) {
			Get();
			PARAM_COMA();
		}
}

void Parser::RETORNO() {
		Expect(21 /* "return" */);
		EXP();
		Expect(10 /* ";" */);
}

void Parser::PARAM_COMA() {
		wchar_t* name; int type; int can; 
		TIPO(type);
		IDENTI(name);
		action->addLocal(name, type, para);
		action->addParameter(type); 
}

void Parser::EXP() {
		int term; 
		TERMINO();
		if (la->kind == 22 /* "-" */ || la->kind == 23 /* "+" */) {
			TERM_OP(term);
			action->addStackOpe(term); 
			EXP();
		}
		action->createCuadroOpeTerm(); 
}

void Parser::VARCTE() {
		double signo=1.00; double number; Direction *dir; int tipo;
		if (la->kind == 22 /* "-" */ || la->kind == 23 /* "+" */) {
			if (la->kind == 22 /* "-" */) {
				Get();
				signo=-1; 
			} else {
				Get();
				
			}
		}
		if (la->kind == _integer) {
			Get();
			number=wcstof((coco_string_create(t->val)), NULL); 
			tipo=entero; 
		} else if (la->kind == _floating) {
			Get();
			number=wcstof((coco_string_create(t->val)), NULL);
			tipo=flotante; 
		} else SynErr(68);
		dir=action->memory->save(3,tipo, number*signo);
		action->dir->push(dir); 
}

void Parser::LECTURA() {
		wchar_t* name; Variable *obj; 
		Expect(29 /* "read" */);
		Expect(17 /* "(" */);
		IDENTI(name);
		obj=action->find(name);
		action->createCuadrRead(obj->dir->direction, obj->type); 
		Expect(18 /* ")" */);
		Expect(10 /* ";" */);
}

void Parser::GRAFICO() {
		int figure; 
		Expect(32 /* "draw" */);
		FIGURA(figure);
		action->beginDraw(figure); 
		Expect(33 /* ":" */);
		while (StartOf(2)) {
			ATRIBUTOS();
		}
		Expect(34 /* "end" */);
		action->endDraw();
}

void Parser::ASIGMODULO() {
		wchar_t* name; Variable *obj; 
		IDENTI(name);
		if (la->kind == 17 /* "(" */) {
			action->findFunction(name); 
			LLAMAR_MODULO();
		} else if (la->kind == 11 /* "[" */ || la->kind == 26 /* "=" */) {
			obj=action->find(name);
			action->addStackDir(obj->dir->direction,obj->type); 
			if (la->kind == 11 /* "[" */) {
				action->getDimensionId(obj); 
				ARREGLO_LLAM();
			}
			ASIGNACION();
		} else SynErr(69);
}

void Parser::CONDICION() {
		Expect(24 /* "if" */);
		Expect(17 /* "(" */);
		EXPRESION();
		Expect(18 /* ")" */);
		action->createGotoFalseCond(); 
		Expect(6 /* "{" */);
		while (StartOf(1)) {
			ESTATUTO();
		}
		Expect(7 /* "}" */);
		if (la->kind == 25 /* "else" */) {
			Get();
			action->createGotoFinCond(); 
			Expect(6 /* "{" */);
			while (StartOf(1)) {
				ESTATUTO();
			}
			Expect(7 /* "}" */);
		}
		action->createEndCond(); 
}

void Parser::ESCRITURA() {
		Expect(30 /* "print" */);
		Expect(17 /* "(" */);
		EXP();
		action->createCuadrPrint(); 
		while (la->kind == 31 /* "|" */) {
			Get();
			EXP();
			action->createCuadrPrint(); 
		}
		Expect(18 /* ")" */);
		Expect(10 /* ";" */);
}

void Parser::CICLO() {
		Expect(27 /* "while" */);
		action->addStackLeapActual(); 
		Expect(17 /* "(" */);
		EXPRESION();
		Expect(18 /* ")" */);
		action->createGotoFalWhile(); 
		Expect(6 /* "{" */);
		while (StartOf(1)) {
			ESTATUTO();
		}
		action->createEndWhile(); 
		Expect(7 /* "}" */);
}

void Parser::CICLO_DO() {
		Expect(28 /* "do" */);
		action->addStackLeapActual(); 
		Expect(6 /* "{" */);
		while (StartOf(1)) {
			ESTATUTO();
		}
		Expect(7 /* "}" */);
		Expect(27 /* "while" */);
		Expect(17 /* "(" */);
		EXPRESION();
		Expect(18 /* ")" */);
		action->createWhileDo(); 
		Expect(10 /* ";" */);
}

void Parser::EXPRESION() {
		int log; 
		EXP_LOG();
		while (la->kind == 59 /* "and" */ || la->kind == 60 /* "or" */) {
			LOG_OPE(log);
			action->addStackOpe(log); 
			EXP_LOG();
		}
		action->createCuadroOpeLog(); 
}

void Parser::EXP_LOG() {
		int rel; 
		EXP();
		if (StartOf(3)) {
			RELACIONAL(rel);
			action->addStackOpe(rel); 
			EXP();
		}
		action->createCuadroOpeRel(); 
}

void Parser::LOG_OPE(int &log) {
		if (la->kind == 59 /* "and" */) {
			Get();
			log=log_and;
		} else if (la->kind == 60 /* "or" */) {
			Get();
			log=log_or; 
		} else SynErr(70);
}

void Parser::RELACIONAL(int &rel) {
		switch (la->kind) {
		case 53 /* "==" */: {
			Get();
			rel=eq;
			break;
		}
		case 54 /* "<>" */: {
			Get();
			rel=ne;
			break;
		}
		case 55 /* "<=" */: {
			Get();
			rel=lte;
			break;
		}
		case 56 /* ">=" */: {
			Get();
			rel=gte;
			break;
		}
		case 57 /* "<" */: {
			Get();
			rel=lt;
			break;
		}
		case 58 /* ">" */: {
			Get();
			rel=gt;;
			break;
		}
		default: SynErr(71); break;
		}
}

void Parser::LLAMAR_MODULO() {
		Expect(17 /* "(" */);
		action->createERA(); 
		if (StartOf(4)) {
			EXP();
			action->createParameter(); 
			while (la->kind == 9 /* "," */) {
				Get();
				EXP();
				action->createParameter(); 
			}
		}
		action->checkParameter(); 
		Expect(18 /* ")" */);
		action->createGotoSub();
		Expect(10 /* ";" */);
}

void Parser::ARREGLO_LLAM() {
		Expect(11 /* "[" */);
		EXP();
		action->generateVerifica(); 
		while (la->kind == 9 /* "," */) {
			Get();
			EXP();
			action->generateVerifica(); 
		}
		Expect(13 /* "]" */);
}

void Parser::ASIGNACION() {
		wchar_t* name; 
		Expect(26 /* "=" */);
		EXP();
		Expect(10 /* ";" */);
		action->createCuadrAlloca(); 
}

void Parser::PARENTESIS() {
		Expect(17 /* "(" */);
		action->addFake();
		action->createERA(); 
		if (StartOf(4)) {
			EXP();
			action->createParameter(); 
			while (la->kind == 9 /* "," */) {
				Get();
				EXP();
				action->createParameter(); 
			}
		}
		action->checkParameter(); 
		Expect(18 /* ")" */);
		action->removeFake();
		action->createGotoSub();  
}

void Parser::TERMINO() {
		int fact; 
		FACTOR();
		if (la->kind == 61 /* "*" */ || la->kind == 62 /* "/" */ || la->kind == 63 /* "%" */) {
			FACTOR_OP(fact);
			action->addStackOpe(fact); 
			TERMINO();
		}
		action->createCuadroOpeFact(); 
}

void Parser::TERM_OP(int &term) {
		if (la->kind == 23 /* "+" */) {
			Get();
			term=plus;
		} else if (la->kind == 22 /* "-" */) {
			Get();
			term=minus;
		} else SynErr(72);
}

void Parser::FACTOR() {
		if (la->kind == _id) {
			IDENTIFICADOR_I();
		} else if (la->kind == 17 /* "(" */) {
			Get();
			action->addFake(); 
			EXPRESION();
			Expect(18 /* ")" */);
			action->removeFake(); 
		} else if (StartOf(5)) {
			VARCTE();
		} else SynErr(73);
}

void Parser::FACTOR_OP(int &fact) {
		if (la->kind == 61 /* "*" */) {
			Get();
			fact=multiply;
		} else if (la->kind == 62 /* "/" */) {
			Get();
			fact=divide;
		} else if (la->kind == 63 /* "%" */) {
			Get();
			fact=module;
		} else SynErr(74);
}

void Parser::IDENTIFICADOR_I() {
		wchar_t* name; Variable *obj; int signo; Direction *dir; 
		IDENTI(name);
		if (StartOf(6)) {
			obj=action->find(name); 
			action->addStackDir(obj->dir->direction,obj->type); 
			if (la->kind == 11 /* "[" */) {
				action->getDimensionId(obj); 
				ARREGLO_LLAM();
			}
		} else if (la->kind == 17 /* "(" */) {
			action->findFunction(name); 
			PARENTESIS();
		} else SynErr(75);
}

